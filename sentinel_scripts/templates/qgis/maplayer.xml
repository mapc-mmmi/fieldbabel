  <maplayer minimumScale="0" maximumScale="1e+08" type={render_type} {render_config} hasScaleBasedVisibilityFlag="0">
    <id>{layer_id}</id>
    <datasource>{layer_datasource}</datasource>
    <layername>{layer_name}</layername>
    <srs>
      <spatialrefsys>
        <proj4>+proj=utm +zone=32 +datum=WGS84 +units=m +no_defs</proj4>
      </spatialrefsys>
    </srs>
{maplayer_configuration}
  </maplayer>
